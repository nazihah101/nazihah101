<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.admin.mydailyexpenses.HomeActivity"
    tools:showIn="@layout/activity_home"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hi, Welcome to E-Finance"
        tools:layout_editor_absoluteX="113dp"
        tools:layout_editor_absoluteY="30dp" />

    <Button
        android:id="@+id/btnChapter"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:onClick="fnChapter"
        android:text="Finance Chapter"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="88dp" />

    <Button
        android:id="@+id/btnQuiz"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:onClick="fnQuiz"
        android:text="Quiz1111"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="160dp" />

</android.support.constraint.ConstraintLayout>
